{
  "manifest_version": 3,
  "name": "Reading time",
  "version": "1.0",
  "description": "Add new words from articles",

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus" 
	],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "sample.js"
  }
}